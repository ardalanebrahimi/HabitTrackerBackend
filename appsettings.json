{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Jwt": {
    "Key": "{JWT_SECRET}",
    "Issuer": "{YOUR_API_URL}"
  },
  "Authentication": {
    "Google": {
      "ClientId": "{GOOGLE_CLIENT_ID}",
      "ClientSecret": "{GOOGLE_CLIENT_SECRET}"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "{POSTGRESQL_CONNECTION_STRING}"
  },
  "OpenAI": {
    "ApiKey": "{OPEN_AI_API}"
  },
  "GooglePlayBilling": {
    "ServiceAccountJsonPath": "{GOOGLE_PLAY_SERVICE_ACCOUNT_JSON_PATH}",
    "ServiceAccountJson": "{GOOGLE_PLAY_SERVICE_ACCOUNT_JSON_CONTENT}",
    "ApplicationName": "HabitTracker",
    "PackageName": "{YOUR_ANDROID_PACKAGE_NAME}"
  },
  "AllowedHosts": "*"
}
